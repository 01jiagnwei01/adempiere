	<queryString>
		<![CDATA[SELECT partner.employee_code AS "Employee Code", insurance.rundate,
partner.name AS "Employee Name", dept.name AS "Department", ROUND(insurance.amount) AS "Amount",
designation.name AS "Designation",  insurance.name AS "LIC NO"
FROM HR_Insurance_History insurance
	JOIN C_BPartner partner ON insurance.C_BPartner_ID = partner.C_BPartner_ID
	JOIN HR_Employee_Insurance empins ON empins.hr_insurance_type_id = insurance.hr_insurance_type_id
        JOIN HR_Employee_Type emptype ON partner.HR_Employee_Type_ID=emptype.HR_Employee_Type_ID
        JOIN HR_Department dept ON dept.HR_Department_ID=partner.HR_Department_ID
        JOIN HR_Designation designation ON partner.HR_Designation_ID=designation.HR_Designation_ID
WHERE
(CASE WHEN $P!{HR_Employee_Type_ID} = 0 THEN 1=1 ELSE emptype.HR_Employee_Type_ID=$P!{HR_Employee_Type_ID} END)
AND insurance.HR_Period_ID=$P!{HR_Period_ID}]]>
	</queryString>
