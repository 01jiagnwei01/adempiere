	<parameter name="FromDate" class="java.sql.Timestamp"/>
	<parameter name="ToDate" class="java.sql.Timestamp"/>
	<queryString>
		<![CDATA[SELECT cbp.name AS EmployeeName ,
       dep.name AS DepartmentName ,
       ( getNumberOfWorkingDays($P{FromDate} ::Date ,$P{ToDate} ::Date) ) AS NumberofWorkingDays ,
       ( getprestDays($P{FromDate} ::Date ,$P{ToDate} ::Date, cbp.C_BPartner_ID ) ) AS PresentDays ,
       ( getNumberDaysRecordedInTimeSheet($P{FromDate} ::Date ,$P{ToDate} ::Date, cbp.C_BPartner_ID ) ) AS timeSheetDays ,
       (getnonbusinessdays($P{FromDate},$P{ToDate} ) ) AS nonworkingdays,
       ( getNumberofLeavesAvailed($P{FromDate} ::Date ,$P{ToDate} ::Date, cbp.C_BPartner_ID) ) AS leavesAvailed
    FROM C_BPartner cbp
    JOIN HR_Department dep ON cbp.HR_Department_ID = dep.HR_Department_ID
GROUP BY cbp.C_BPartner_ID , cbp.HR_Department_ID ,cbp.name , dep.name
ORDER BY dep.name]]>
	</queryString>
