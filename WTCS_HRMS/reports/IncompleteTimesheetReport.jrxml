	<field name="timesheetdays" class="java.math.BigDecimal"/>
	<field name="leavesavailed" class="java.math.BigDecimal"/>
	<field name="nonworkingdays" class="java.math.BigDecimal"/>
	<variable name="Incompletedays" class="java.math.BigDecimal">
		<variableExpression><![CDATA[($F{numberofworkingdays}.subtract( $F{timesheetdays}.add($F{leavesavailed} ) ) ).compareTo(BigDecimal.ZERO) < 0 ?  BigDecimal.ZERO : ($F{numberofworkingdays}.subtract( $F{timesheetdays}.add($F{leavesavailed} ) ) )]]></variableExpression>
