	<queryString>
		<![CDATA[SELECT
       cs.tax_division As Division,
       cs.tax_circle As Circle,
       cs.registration_number AS Tin,
       cl.name AS ClientName,
       cp.name AS ReturnMonth ,
       trs.datesubmit AS SubmittedOn ,
       trs.grandtotal AS GrandTotal,
       trs.ispaid AS ispaid,
       trs.carryforward AS carryForward

 FROM WTC_TaxIndicator tind
 JOIN WTC_ClientStatutory cs ON cs.WTC_ClientStatutory_ID=tind.WTC_ClientStatutory_ID
 JOIN AD_Client cl  ON cs.AD_Client_ID = cl.AD_Client_ID
 JOIN WTC_TaxreturnSubmission trs ON ((trs.WTC_TaxIndicator_ID = tind.WTC_TaxIndicator_ID) AND (trs.AD_Client_ID = cl.AD_Client_ID ))
 JOIN C_Period cp ON cp.C_Period_ID = trs.C_Period_ID
 WHERE
 tind.WTC_TaxIndicator_ID= $P{WTC_TaxIndicator_ID} AND  cp.C_Year_ID=$P{C_Year_ID};]]>
	</queryString>
