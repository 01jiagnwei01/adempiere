	<queryString><![CDATA[SELECT fine.finedate as fromdate, 
       COALESCE(cb.name,'') as employeename,
       COALESCE(cb.employee_code,'') as employeenumber,
       COALESCE(fine.fineamount,0) as fineamount ,
       (CASE WHEN ($P{HR_Employee_Type_ID}=0) THEN 'ALL' ELSE (SELECT name FROM HR_Employee_Type WHERE 
        HR_Employee_Type_ID=$P{HR_Employee_Type_ID})  END) AS EmployeeType  

FROM hr_emp_fine  fine JOIN C_BPartner cb ON cb.c_bpartner_id=fine.c_bpartner_id 
WHERE 
(CASE WHEN $P{C_BPartner_ID} = 0 THEN 1=1  ELSE fine.C_BPartner_id=$P{C_BPartner_ID} END)
AND 
 (CASE WHEN $P{HR_Employee_Type_ID} = 0 THEN 1 = 1 ELSE cb.HR_Employee_Type_ID=$P{HR_Employee_Type_ID} end) 

AND
 fine.finedate between '$P!{FromDate}' AND '$P!{ToDate}']]></queryString>

