	<queryString>
		<![CDATA[select c.legal_name,cp.name as periodname, cbp.name,coalesce(bpacc.accountno,''),ps.netsalary from hr_payslip ps
join c_bpartner cbp on cbp.c_bpartner_id = ps.c_bpartner_id
join c_bp_bankaccount bpacc on bpacc.c_bpartner_id = cbp.c_bpartner_id
JOIN AD_Client c on c.ad_client_id = ps.ad_client_id
JOIN HR_Period hp ON hp.hr_period_id = ps.hr_period_id
JOIN C_Period cp ON hp.c_period_id = cp.c_period_id
where  ps.hr_period_id =$P{HR_Period_ID}]]>
	</queryString>
