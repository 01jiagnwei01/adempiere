	<queryString>
		<![CDATA[SELECT cb.employee_code AS EMPNO,
        cb.name AS NAME,
        hd.name as DESIGNATION,
        (SELECT name FROM AD_Org WHERE AD_Org_ID=$P{AD_Org_ID} ) AS Division,
        (CASE WHEN ($P{HR_Employee_Type_ID}=0) THEN 'ALL' ELSE (SELECT name FROM HR_Employee_Type WHERE HR_Employee_Type_ID=$P{HR_Employee_Type_ID})   END) AS EmployeeTypeName,
        (SELECT date_part('year',startdate)  FROM HR_Period  WHERE HR_Period_ID=(SELECT  HR_Period_ID FROM HR_Process WHERE HR_Process_ID=$P{HR_Process_ID} ) ) AS curryear ,
        (SELECT to_char(startdate,'MONTH') FROM HR_Period WHERE HR_Period_ID=(SELECT  HR_Period_ID FROM HR_Process WHERE HR_Process_ID=$P{HR_Process_ID} ) ) AS currmonth ,
        (SELECT COALESCE(hrm.amount,0) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Effective Salary Days') )
         ) AS PresentDays ,
        (SELECT (Case WHEN cb.Hr_Employee_Type_ID =(SELECT HR_Employee_Type_ID FROM HR_Employee_Type WHERE UPPER(name) like ('STAFF') )
                      THEN ( hre.monthly_salary )
                      ELSE ( hre.daily_salary ) END)
         FROM HR_Employee hre WHERE hre.C_BPartner_ID=cb.C_BPartner_ID
        ) AS Salary ,
        (SELECT round(COALESCE(hrm.qty,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Working Days') )
        ) AS WorkingDays,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Earned Salary') )
        ) AS EarnedSalary,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Attendance Incentive') )
        ) AS AttendanceIncentive,
                (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='OnTime Incentive') )
        ) AS OnTimeIncentive,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='ThreeMonthIncentive') )
        ) AS ThreeMonthIncentive,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='OverTime Incentive') )
        ) AS OverTimeIncentive,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='WeeklyIncentive') )
        ) AS WeeklyIncentive,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Insurance') )
        ) AS LIC,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Advance Payment') )
        ) AS AdvancePayment,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Canteen') )
        ) AS CANTEEN,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='StoreAmount') )
        ) AS StoreAmount,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Fine Amount') )
        ) AS FineAmount,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='GrossSalary') )
        ) AS GrossSalary,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='GrossDeductions') )
        ) AS GrossDeductions,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Back Logs') )
        ) AS BackLogs,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Other Earnings') )
        ) AS OtherEarnings,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Other Deductions') )
        ) AS OtherDeductions,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Power') )
        ) AS powerBill,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Dish') )
        ) AS DishBill ,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='TDS') )
        ) AS TDS,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='ShiftChangeAbsentDeduction') )
        ) AS ShiftChangeAbsentDeduction,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID=$P{HR_Process_ID}
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='LateCommingDeduction') )
        ) AS LateComingDeduction
 FROM C_BPartner cb
 JOIN HR_Designation hd ON cb.HR_Designation_ID=hd.HR_Designation_ID
 JOIN HR_Employee hre ON ( hre.C_BPartner_ID=cb.C_BPartner_ID ) AND (hre.hasoptedpf='N')
 JOIN Hr_Employee_Type ety ON (cb.Hr_Employee_Type_ID=ety.Hr_Employee_Type_ID) AND (CASE WHEN ($P{HR_Employee_Type_ID}=0) THEN (1=1) ELSE (cb.Hr_Employee_Type_ID=$P{HR_Employee_Type_ID}) END)
 WHERE
 (cb.ad_org_ID =$P{AD_Org_ID})
AND
 (CASE WHEN (cb.relievingdate IS NULL) THEN (cb.relieveemployee='N') ELSE (cb.relievingdate > (SELECT enddate FROM HR_Period WHERE HR_Period_ID=(SELECT  HR_Period_ID FROM HR_Process WHERE HR_Process_ID=$P{HR_Process_ID} ))) END )
AND
 ( cb.isemployee='Y' )
AND
 (cb.joiningdate<=(SELECT enddate FROM HR_Period WHERE HR_Period_ID=(SELECT HR_Period_ID FROM HR_Process WHERE HR_Process_ID=$P{HR_Process_ID}) ))
AND
  EXISTS (SELECT 1 FROM HR_Movement hrm WHERE C_BPartner_ID = cb.C_BPartner_ID AND HR_Process_ID = $P{HR_Process_ID})]]>
	</queryString>
