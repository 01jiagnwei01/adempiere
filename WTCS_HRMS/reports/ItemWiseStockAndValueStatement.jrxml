	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="AD_Org_ID" class="java.math.BigDecimal"/>
	<parameter name="FromDate" class="java.sql.Timestamp"/>
	<parameter name="ToDate" class="java.sql.Timestamp"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["./"]]></defaultValueExpression>
	</parameter>
	<parameter name="M_Product_Category_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[new BigDecimal(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="C_AcctSchema_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT COALESCE(pro.value,'---') AS ItemCode,
       COALESCE(pro.name,'---') AS ItemName,
       COALESCE(drawing.drawing_number,'---') AS DrawingNumber ,
       COALESCE(cat.catalog_number,'---') AS ItemCatalog ,
       COALESCE(uom.name,'---') AS UOM ,
       COALESCE(catagory.name,'---') AS ItemCategory ,
       COALESCE((SELECT SUM(COALESCE(mcd1.Qty,0))/( SELECT    COUNT(DISTINCT (mcd11.M_CostElement_ID ))
                                                     FROM M_CostDetail mcd11
                                                     WHERE  (mcd11.dateacct <'$P!{FromDate}')
                                                     AND (pro.M_Product_ID=mcd11.M_Product_ID )
                                                     AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE pro.M_Product_Category_ID=$P{M_Product_Category_ID} END)
                                                     AND (mcd11.C_AcctSchema_ID = $P{C_AcctSchema_ID})
                                                     AND mcd11.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                                     AND mcd11.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                                  )
                FROM M_CostDetail mcd1
                 WHERE  (mcd1.dateacct <'$P!{FromDate}')
                  AND (pro.M_Product_ID=mcd1.M_Product_ID )
                  AND (CASE WHEN 0 = 0 THEN 1=1 ELSE pro.M_Product_Category_ID=0 END)
                  AND (mcd1.C_AcctSchema_ID = $P{C_AcctSchema_ID})
                  AND mcd1.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                  AND mcd1.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )  )
       ,0     ) AS OpenQty ,
       COALESCE((SELECT SUM(COALESCE(mcd2.Amt,0)) from M_CostDetail mcd2
                                         WHERE  (mcd2.dateacct <'$P!{FromDate}')
                                         AND (pro.M_Product_ID=mcd2.M_Product_ID )
                                         AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE pro.M_Product_Category_ID=$P{M_Product_Category_ID} END)
                                         AND (mcd2.C_AcctSchema_ID = $P{C_AcctSchema_ID})
                                         AND mcd2.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                         AND mcd2.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
       ),0) AS OpenValue ,
       COALESCE((SELECT SUM(COALESCE(mcd3.Qty,0))/( SELECT    COUNT(DISTINCT (mcd31.M_CostElement_ID ))
                                                       FROM M_CostDetail mcd31
                                                       WHERE  (mcd31.dateacct BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                                                       AND (pro.M_Product_ID=mcd31.M_Product_ID )
                                                       AND (mcd31.Qty > 0)
                                                       AND mcd31.C_OrderLine_ID IS NOT NULL
                                                       AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE pro.M_Product_Category_ID=$P{M_Product_Category_ID} END)
                                                       AND (mcd31.C_AcctSchema_ID = $P{C_AcctSchema_ID})
                                                       AND mcd31.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                                       AND mcd31.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                                   )
                FROM M_CostDetail mcd3
                WHERE  (mcd3.dateacct BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                  AND (pro.M_Product_ID=mcd3.M_Product_ID )
                  AND (mcd3.Qty > 0)
                  AND mcd3.C_OrderLine_ID IS NOT NULL
                  AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE pro.M_Product_Category_ID=$P{M_Product_Category_ID} END)
                  AND (mcd3.C_AcctSchema_ID = $P{C_AcctSchema_ID})
                  AND mcd3.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                  AND mcd3.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )  )
       ,0     ) AS PurchaseQty ,
       COALESCE((SELECT SUM(COALESCE(mcd4.Amt,0)) from M_CostDetail mcd4
                                         WHERE  (mcd4.dateacct BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                                         AND (pro.M_Product_ID=mcd4.M_Product_ID )
                                         AND (mcd4.Amt > 0)
                                         AND mcd4.C_OrderLine_ID IS NOT NULL
                                         AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE pro.M_Product_Category_ID=$P{M_Product_Category_ID} END)
                                         AND (mcd4.C_AcctSchema_ID = $P{C_AcctSchema_ID})
                                         AND mcd4.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                         AND mcd4.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
       ),0) AS PurchaseValue ,
        COALESCE((SELECT SUM(ll.qty)  AS consumedqty
         FROM ( SELECT  DISTINCT mi.movementdate AS IndentDate ,
                        mpc.name As ProductCategory ,
                        COALESCE((
                          SELECT SUM(COALESCE(mcd2.qty,0)) /( SELECT ( CASE WHEN ( COUNT(DISTINCT (mcd51.M_CostElement_ID )) ) = 0
                                                                            THEN 1
                                                                            ELSE ( COUNT(DISTINCT (mcd51.M_CostElement_ID )) )
                                                                       END
                                                                     )
                                                               FROM M_CostDetail mcd51
                                                               WHERE  (mcd51.dateacct BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                                                               AND (pro.M_Product_ID=mcd51.M_Product_ID )
                                                               AND (mcd51.Qty > 0)
                                                               AND (CASE
                                                                    WHEN $P{M_Product_Category_ID} = 0
                                                                    THEN 1=1
                                                                    ELSE pro.M_Product_Category_ID=$P{M_Product_Category_ID}
                                                                    END
                                                                   )
                                                               AND (mcd51.C_AcctSchema_ID = $P{C_AcctSchema_ID})
                                                               AND mcd51.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                                               AND mcd51.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
                                                             )
                          from M_CostDetail mcd2
                          WHERE  (mcd2.dateacct BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                              AND (mp.M_Product_ID=mcd2.M_Product_ID )
                              AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE mp.M_Product_Category_ID =$P{M_Product_Category_ID} END )
                              AND mcd2.M_InventoryLine_ID=mil.M_InventoryLine_ID
                              AND (mcd2.C_AcctSchema_ID =  $P{C_AcctSchema_ID})
                              AND mcd2.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID =  $P{C_AcctSchema_ID} )
                              AND mcd2.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID =  $P{C_AcctSchema_ID} )
                        ),0) AS qty
                FROM  M_Inventory mi
                JOIN  M_InventoryLine mil ON mil.M_Inventory_ID=mi.M_Inventory_ID
                JOIN  M_Product mp ON ( mil.M_Product_ID=mp.M_Product_ID ) AND (mp.M_Product_ID = pro.M_Product_ID )
                JOIN  M_Product_Category  mpc ON mpc.M_Product_Category_ID=mp.M_Product_Category_ID
                Where ( mi.movementdate BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                AND mi.movementtype IN ('I-' , 'I+')
                AND mi.docstatus IN ('CO','CL')
                AND mil.currentcostprice =0
                AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE mpc.M_Product_Category_ID =$P{M_Product_Category_ID} END )
                group by mi.movementdate,mpc.name,mp.M_Product_ID,mpc.M_Product_Category_ID,mil.M_InventoryLine_ID ,mp.M_Product_Category_ID
             ) ll
       ),0) AS consumedqty ,
        COALESCE((SELECT SUM(ll.Amount)  AS ConsumedValue
         FROM ( SELECT  DISTINCT mi.movementdate AS IndentDate ,
                        mpc.name As ProductCategory ,
                        COALESCE((
                                 SELECT SUM(COALESCE(mcd2.Amt,0))
                                 from M_CostDetail mcd2
                                 WHERE  (mcd2.dateacct BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                                  AND (mp.M_Product_ID=mcd2.M_Product_ID )
                                  AND (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE mp.M_Product_Category_ID =$P{M_Product_Category_ID} END )
                                  AND mcd2.M_InventoryLine_ID=mil.M_InventoryLine_ID
                                  AND (mcd2.C_AcctSchema_ID =  $P{C_AcctSchema_ID})
                                  AND mcd2.M_Costtype_ID IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID =  $P{C_AcctSchema_ID} )
                                  AND mcd2.costingmethod IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID =  $P{C_AcctSchema_ID} )
                        ),0) AS Amount
                FROM  M_Inventory mi
                JOIN  M_InventoryLine mil ON mil.M_Inventory_ID=mi.M_Inventory_ID
                JOIN  M_Product mp ON mil.M_Product_ID=mp.M_Product_ID AND (mp.M_Product_ID = pro.M_Product_ID )
                JOIN  M_Product_Category  mpc ON mpc.M_Product_Category_ID=mp.M_Product_Category_ID
                Where ( mi.movementdate BETWEEN '$P!{FromDate}' and '$P!{ToDate}')
                AND mi.movementtype IN ('I-' , 'I+')
                AND mi.docstatus IN ('CO','CL')
                AND mil.currentcostprice =0
                group by mi.movementdate,mpc.name,mp.M_Product_ID,mpc.M_Product_Category_ID,mil.M_InventoryLine_ID , mp.M_Product_Category_ID
             ) ll
       ),0) AS ConsumedValue

FROM M_product pro
LEFT OUTER JOIN WTC_Drawing_Number drawing ON pro.WTC_Drawing_Number_ID=drawing.WTC_Drawing_Number_ID
LEFT OUTER JOIN WTC_Item_Catalog_Number cat ON (cat.WTC_Item_Catalog_Number_ID=pro.WTC_Item_Catalog_Number_ID )
LEFT OUTER JOIN M_Product_Category catagory ON catagory.M_Product_Category_ID=pro.M_Product_Category_ID
LEFT OUTER JOIN C_Uom uom ON uom.C_Uom_ID=pro.C_Uom_ID
WHERE  (CASE WHEN $P{M_Product_Category_ID} = 0 THEN 1=1 ELSE pro.M_Product_Category_ID=$P{M_Product_Category_ID} END )]]>
	</queryString>
