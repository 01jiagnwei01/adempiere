	<field name="purchaseqty" class="java.math.BigDecimal"/>
	<field name="purchasevalue" class="java.math.BigDecimal"/>
	<field name="consumedqty" class="java.math.BigDecimal"/>
	<field name="consumedvalue" class="java.math.BigDecimal"/>
	<variable name="TotalStock" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{openqty}.add($F{purchaseqty})]]></variableExpression>
	</variable>
	<variable name="TotalValue" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{openvalue}.add($F{purchasevalue})]]></variableExpression>
	</variable>
	<variable name="ClosingStock" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{TotalStock}.add($F{consumedqty})]]></variableExpression>
	</variable>
	<variable name="ClosingValue" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{TotalValue}.add($F{consumedvalue})]]></variableExpression>
	</variable>
