	<parameter name="C_BPartner_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
cbp.employee_code AS "Employee ID",
cbp.name AS "Employee Name",
lt.leavetype AS "Leave Type",
lr.fromdate AS "From Date",
lr.todate AS "To Date",

lr.number_of_workingdays AS "No Of Days"

 FROM HR_Leave_Request lr
 JOIN C_BPartner cbp ON lr.c_bpartner_id = cbp.c_bpartner_id
 JOIN HR_LeaveType lt ON lr.hr_leavetype_id = lt.hr_leavetype_id
 WHERE cbp.isEmployee = 'Y' and lr.isavailed = 'Y'
 AND
( CASE  when $P{C_BPartner_ID}  is not null then cbp.c_bpartner_id = $P{C_BPartner_ID} else 1=1 end)

 and
 lr.fromdate >=$P{fromdate} and  lr.fromdate <= $P{todate}
 and
 lr.todate >= $P{fromdate} and  lr.todate<=$P{todate}]]>
	</queryString>
