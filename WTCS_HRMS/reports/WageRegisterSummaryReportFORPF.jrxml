	</variable>
	<variable name="GrandFine" class="java.math.BigDecimal" resetType="Group" resetGroup="GroupDepartment" calculation="Sum">
		<variableExpression><![CDATA[$F{fineamount}]]></variableExpression>
	</variable>
	<variable name="GrandPower" class="java.math.BigDecimal" resetType="Group" resetGroup="GroupDepartment" calculation="Sum">
		<variableExpression><![CDATA[$F{powerbill}]]></variableExpression>
