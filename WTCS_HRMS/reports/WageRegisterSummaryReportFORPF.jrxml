 <jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="WageRegisterSummaryReportFORPF" pageWidth="2300" pageHeight="842" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="2300" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="590"/>
	<property name="ireport.y" value="0"/>
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="HR_Period_ID" class="java.math.BigDecimal"/>
	<parameter name="HR_Employee_Type_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[new BigDecimal(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="Parent_Org_ID" class="java.math.BigDecimal"/>
	<parameter name="AD_Org_ID" class="java.math.BigDecimal"/>
	<parameter name="PF_Non_PF" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
        dept.name AS Department ,

        (SELECT name FROM AD_Org WHERE AD_Org_ID=$P{AD_Org_ID} ) AS Division,
        (SELECT round(COALESCE(hrm.amount,0)) FROM HR_Movement hrm
                     WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_Process_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID}  AND  payrollprocesstype='General')
                     AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Effective Salary Days') )
         ) AS PresentDays ,
        (SELECT (Case WHEN cb.Hr_Employee_Type_ID =(SELECT HR_Employee_Type_ID FROM HR_Employee_Type WHERE UPPER(name) like ('STAFF') )
                      THEN ( hre.monthly_salary )
                      ELSE ( hre.daily_salary ) END)
         FROM HR_Employee hre WHERE hre.C_BPartner_ID=cb.C_BPartner_ID
        ) AS Salary ,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
              WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT pro.HR_Process_ID FROM HR_Process pro WHERE pro.HR_Period_ID=$P{HR_Period_ID} AND  pro.payrollprocesstype='General')
              AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Basic') )
        ) AS BasicSalary,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
              WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT pro.HR_Process_ID FROM HR_Process pro WHERE pro.HR_Period_ID=$P{HR_Period_ID} AND  pro.payrollprocesstype='General')
              AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='HRA') )
        ) AS HRA,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='MedicalAllowance') )
        ) AS MedicalAllowance,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Conveyance Allowance') )
        ) AS ConveyanceAllowance,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Special Allowance') )
        ) AS SpecialAllowance,
        (SELECT round(COALESCE(hrm.qty,0)) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Working Days') )
        ) AS WorkingDays,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Earned Salary') )
        ) AS EarnedSalary,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Attendance Incentive') )
        ) AS AttendanceIncentive,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='OnTime Incentive') )
        ) AS OnTimeIncentive,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Three Month Incentive') )
        ) AS ThreeMonthIncentive,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='OverTime Incentive') )
        ) AS OverTimeIncentive,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Weekly Incentive') )
        ) AS WeeklyIncentive,
          (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='PF') )
        ) AS PF,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='ESI') )
        ) AS ESI,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='ProfessionalTax') )
        ) AS ProfessionalTax,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN  (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Insurance') )
        ) AS LIC,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Advance Payment') )
        ) AS AdvancePayment,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Canteen') )
        ) AS CANTEEN,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='StoreAmount') )
        ) AS StoreAmount,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Fine Amount') )
        ) AS FineAmount,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='GrossSalary') )
        ) AS GrossSalary,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='GrossDeductions') )
        ) AS GrossDeductions,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Back Logs') )
        ) AS BackLogs,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Other Earnings') )
        ) AS OtherEarnings,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Other Deductions') )
        ) AS OtherDeductions,
       (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                    WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                    AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Power') )
        ) AS powerBill,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Dish') )
        ) AS DishBill  ,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='TDS') )
        ) AS TDS,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID  IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='ShiftChangeAbsentDeduction') )
        ) AS ShiftChangeAbsentDeduction,
        (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Late Coming Deduction') )
        ) AS LateComingDeduction ,

       (SELECT round(COALESCE(hrm.amount,0),2) FROM HR_Movement hrm
                           WHERE  hrm.C_BPartner_ID=cb.C_BPartner_ID  AND hrm.HR_PRocess_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND payrollprocesstype='General')
                           AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='PartialNetAmount') )
        ) AS PartialNetAmount
 FROM C_BPartner cb
 JOIN HR_Designation hd ON cb.HR_Designation_ID=hd.HR_Designation_ID
 JOIN HR_Employee hre ON ( hre.C_BPartner_ID=cb.C_BPartner_ID ) AND (hre.hasoptedpf='Y')
 JOIN Hr_Employee_Type ety ON (cb.HR_Employee_Type_ID=ety.HR_Employee_Type_ID) AND (CASE WHEN ($P{HR_Employee_Type_ID}=0) THEN (1=1) ELSE (cb.Hr_Employee_Type_ID=$P{HR_Employee_Type_ID}) END)
 JOIN HR_Department dept ON (dept.HR_Department_ID=cb.HR_Department_ID)
 WHERE  (cb.ad_org_ID =$P{AD_Org_ID})
AND
  (CASE WHEN (cb.relievingdate IS NULL) THEN (cb.relieveemployee='N') ELSE (cb.relievingdate > (SELECT enddate FROM HR_Period WHERE HR_Period_ID=$P{HR_Period_ID})) END )
AND
  ( cb.isemployee='Y' )
AND
  (cb.joiningdate<=(SELECT enddate FROM HR_Period WHERE HR_Period_ID=$P{HR_Period_ID} ))
AND
  (hre.hasoptedpf='Y')
AND
   EXISTS (SELECT 1 FROM HR_Movement hrm WHERE C_BPartner_ID = cb.C_BPartner_ID AND HR_Process_ID IN (SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID = $P{HR_Period_ID}))
 Order by Department]]>
	</queryString>
