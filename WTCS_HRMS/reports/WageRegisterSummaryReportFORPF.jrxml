	</variable>
	<variable name="GrandWeekly" class="java.math.BigDecimal" resetType="Group" resetGroup="GroupDepartment" calculation="Sum">
		<variableExpression><![CDATA[$F{weeklyincentive}]]></variableExpression>
	</variable>
	<variable name="GrandGrossEarnings" class="java.math.BigDecimal" resetType="Group" resetGroup="GroupDepartment" calculation="Sum">
		<variableExpression><![CDATA[$F{grosssalary}]]></variableExpression>
	</variable>
	<variable name="GrandPF" class="java.math.BigDecimal" resetType="Group" resetGroup="GroupDepartment" calculation="Sum">
		<variableExpression><![CDATA[$F{pf}]]></variableExpression>
	</variable>
