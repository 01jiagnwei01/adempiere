	<subDataset name="attendence" >

		<queryString><![CDATA[SELECT cct.name AS CategoryName,cc.name AS ConceptName,hrm.qty,hrm.HR_Process_ID
FROM HR_Movement hrm
JOIN HR_Concept_Category cc ON (cc.HR_Concept_Category_ID = hrm.HR_Concept_Category_ID)
JOIN HR_Concept cct ON (cct.HR_Concept_ID = hrm.HR_Concept_ID)
WHERE hrm.C_BPartner_ID=1000030 AND hrm.HR_PRocess_ID=1000009 AND 
(hrm.HR_Concept_Category_ID =(SELECT HR_Concept_Category_ID FROM HR_Concept_Category hrcc WHERE hrcc.name='Attendence' ))
AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Working Days' OR 
hrc.name='Absent Days' OR hrc.name='Weekly off' OR  hrc.name='Present Days' OR
  hrc.name='Late Comming'))]]></queryString>

		<field name="categoryname" class="java.lang.String"/>
		<field name="conceptname" class="java.lang.String"/>
		<field name="qty" class="java.math.BigDecimal"/>
		<field name="hr_process_id" class="java.math.BigDecimal"/>

	</subDataset>

	<parameter name="C_BPartner_ID" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="HR_Process_ID" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[SELECT CASE 
WHEN cct.name='ActualPresentedDays' THEN 'Actual Present Days' 
WHEN cct.name='PresentedWeeklyOffs' THEN 'Presented Weekly Offs'
ELSE cct.name END AS CategoryName, 
cc.name AS ConceptName, 
(CASE WHEN cct.columntype='A' Then (COALESCE(hrm.amount,0)) ELSE (COALESCE(hrm.qty,0)) END) AS qty, hrm.HR_Process_ID
FROM HR_Movement hrm
JOIN HR_Concept_Category cc ON (cc.HR_Concept_Category_ID = hrm.HR_Concept_Category_ID)
JOIN HR_Concept cct ON (cct.HR_Concept_ID = hrm.HR_Concept_ID)
WHERE hrm.C_BPartner_ID=$P{C_BPartner_ID} AND hrm.HR_Process_ID=$P{HR_Process_ID} AND 
(hrm.HR_Concept_Category_ID =(SELECT HR_Concept_Category_ID FROM HR_Concept_Category hrcc WHERE hrcc.name='Attendence' ))
AND (hrm.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Working Days' OR 
hrc.name='Absent Days' OR hrc.name='Weekly off' OR  hrc.name='Present Days' OR hrc.name='Late Comming' OR hrc.name='PresentedWeeklyOffs' OR hrc.name='ActualPresentedDays'))]]></queryString>

