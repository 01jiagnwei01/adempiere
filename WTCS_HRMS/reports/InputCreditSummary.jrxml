 <jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="InputCreditSummary" language="java" pageWidth="612" pageHeight="792" whenNoDataType="NoDataSection" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="WTC_TaxIndicator_ID" class="java.math.BigDecimal"/>
	<parameter name="C_Year_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[java.math.BigDecimal.ZERO]]></defaultValueExpression>
	</parameter>
	<parameter name="C_Period_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[java.math.BigDecimal.ZERO]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT @ SUM(cit.taxAmt) AS Inputcredit , UPPER( to_char(   c.dateInvoiced , 'month') ) as month,date_part( 'year',  c.dateInvoiced ) as year ,ct.name AS Tax , ti.name AS TaxIndicator FROM
C_InvoiceTax cit
JOIN C_Invoice c ON ( c.C_Invoice_ID = cit.C_Invoice_ID )
JOIN C_Tax ct ON ( ct.C_Tax_ID = cit.C_Tax_ID )
JOIN WTC_TaxIndicator ti ON ( ti.WTC_TaxIndicator_ID = ct.WTC_TaxIndicator_ID )

WHERE

CASE WHEN ti.CollectableOn ='SO' THEN
c.C_DocType_ID IN ( SELECT C_DocType_ID FROM C_DocType WHERE docbasetype IN ('API','ARC') )
ELSE
c.C_DocType_ID IN ( SELECT C_DocType_ID FROM C_DocType WHERE docbasetype IN ('ARI','APC') ) END
AND c.isPaid='Y'
AND ti.WTC_TaxIndicator_ID= $P{WTC_TaxIndicator_ID}
AND
  CASE WHEN $P{C_Period_ID} = 0 THEN

    ( c.dateinvoiced >= (SELECT startdate from C_Period WHERE C_Year_ID=$P{C_Year_ID} and periodno =1)
    AND
     c.dateinvoiced <= (SELECT enddate from C_Period WHERE C_Year_ID=$P{C_Year_ID} and periodno =12) )
  ELSE
   ( c.dateinvoiced >= (SELECT startdate from C_Period WHERE C_Period_ID=$P{C_Period_ID})
    AND
   c.dateinvoiced <= (SELECT enddate from C_Period WHERE C_Period_ID=$P{C_Period_ID}) )
  END
GROUP BY  c.dateinvoiced,ti.name , ct.name]]>
	</queryString>
