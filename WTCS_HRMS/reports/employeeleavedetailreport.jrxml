	<variable name="wdpercentage" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[new BigDecimal(100).subtract($V{SUM_number_of_workingdays_1}.divide(new BigDecimal($F{days}.intValue()),1).multiply(new BigDecimal(100)))]]></variableExpression>
	</variable>
	<variable name="empleavetotal" class="java.math.BigDecimal" resetType="Group" resetGroup="empleavetotal" calculation="Sum">
		<variableExpression><![CDATA[$F{number_of_workingdays}]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
	<variable name="empwdpercentage" class="java.math.BigDecimal" resetType="Group" resetGroup="empleavetotal" calculation="Nothing">
		<variableExpression><![CDATA[new BigDecimal(100).subtract(($V{empleavetotal}.divide($F{days},5,5)).multiply(new BigDecimal(100)))]]></variableExpression>
