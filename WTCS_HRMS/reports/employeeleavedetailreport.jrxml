	<parameter name="date1" isForPrompting="true" class="java.sql.Timestamp"/>
	<parameter name="date2" isForPrompting="true" class="java.sql.Timestamp"/>
	<queryString><![CDATA[SELECT
cbp.name,
lr.fromdate ,
lr.todate,
lr.number_of_workingdays,
lt.leavetype,
COALESCE(lr.otherdisapprovalreason,rs.name),
case when lr.created > lr.fromdate then 'Y' else 'N' end,
(($P{date2}::Date - $P{date1}::Date) + 1 - getNumberOfNonWorkingDays($P{date1},$P{date2}) - getNonBusinessDays($P{date1},$P{date2})) as days

 FROM  hr_leave_request lr
JOIN  c_bpartner cbp ON cbp.c_bpartner_id = lr.c_bpartner_id
JOIN hr_leaveType lt ON lt.hr_leavetype_id = lr.hr_leavetype_id
JOIN wtc_reasons rs on lr.wtc_reasons_id = rs.wtc_reasons_id
WHERE
lr.isavailed = 'Y'
and 
lr.fromdate >=$P{date1} and todate<=$P{date2}

order by cbp.name,lr.fromdate]]></queryString>

