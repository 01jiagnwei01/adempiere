	<parameter name="WorkDate" class="java.sql.Timestamp"/>
	<parameter name="HR_Department_ID" class="java.math.BigDecimal"/>
	<parameter name="HR_Work_Shift_ID" class="java.math.BigDecimal"/>
	<parameter name="AD_Org_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[new BigDecimal(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["./"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT  dep.name AS Department, cb.name AS EmployeeName , cb.employee_code As EmployeeCode ,COALESCE(hq.name,'NO Accommodation') AS QuarterNo ,COALESCE(wg.name,'') AS  WorkGroup ,
ws.name AS WORKShift ,
(Case When (sup.HR_Emp_Gate_Attendence_ID  IS NULL) THEN (Case WHEN (lr.HR_Leave_Request_ID IS NULL)
                                                              THEN (CASE WHEN (CAST(cb.weeklyoff as double precision) ) =(CASE WHEN (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') ) =0
                                                                                                                               THEN 7
                                                                                                                               ELSE (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') )
                                                                                                                          END
                                                                                                                          )
                                                                         THEN 'Weekly Off'
                                                                         ELSE 'Absent' END
                                                                    )
                                                              WHEN (lr.isApproved='N') THEN
                                                                         (CASE WHEN (CAST(cb.weeklyoff as double precision))=(CASE WHEN (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}'))=0
                                                                                                                               THEN 7
                                                                                                                               ELSE (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') )
                                                                                                                               END
                                                                                                                              )
                                                                               THEN 'Weekly Off'
                                                                               ELSE 'Absent' END
                                                                         )
                                                              WHEN  (lr.isApproved='Y') THEN
                                                                         (CASE WHEN (CAST(cb.weeklyoff as double precision))=(CASE WHEN (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}'))=0
                                                                                                                               THEN 7
                                                                                                                               ELSE (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') )
                                                                                                                               END
                                                                                                                              )
                                                                               THEN 'Weekly Off'
                                                                               ELSE  'Leave'  END
                                                                         )
                                                              ELSE 'Absent' END
                                                         )
                  ELSE 'Absent'  END
)  AS Status
FROM C_BPartner cb
               JOIN HR_Work_Group wg  ON (wg.HR_Work_Group_ID=cb.HR_Work_Group_ID) AND (wg.HR_Work_Group_ID=cb.HR_Work_Group_ID)
               JOIN HR_Shift_Alloc alc ON (alc.HR_Work_Group_ID=wg.HR_Work_Group_ID) AND (alc.workdate='$P!{WorkDate}')
               JOIN HR_Work_Shift  ws ON  (ws.HR_Work_Shift_ID=alc.HR_Work_Shift_ID) AND (ws.HR_Work_Shift_ID=$P{HR_Work_Shift_ID})
               LEFT OUTER JOIN HR_Department dep ON  (dep.HR_Department_ID=cb.HR_Department_ID)
               JOIN AD_Org org ON org.AD_Org_ID=cb.division_ID  AND (CASE WHEN $P{AD_Org_ID}=0 THEN ( 1 = 1 ) ELSE cb.division_ID=$P{AD_Org_ID} END)
               LEFT OUTER JOIN HR_Quarter hq ON hq.HR_Quarter_ID=cb.HR_Quarter_ID
               LEFT OUTER JOIN HR_Emp_Gate_Attendence sup ON (sup.C_BPartner_ID=cb.C_BPartner_ID) AND (sup.workdate='$P!{WorkDate}')
                               AND  (sup.actual_work_shift_id=ws.HR_Work_Shift_ID)
               LEFT OUTER JOIN HR_Leave_Request lr ON ( lr.C_BPartner_ID=cb.C_BPartner_ID )  AND ('$P!{WorkDate}' BETWEEN lr.fromdate AND lr.todate)
 WHERE (CASE WHEN (cb.relievingdate IS NULL) THEN (1=1) ELSE (cb.relievingdate > '$P!{WorkDate}') END )  AND ( cb.isemployee='Y' ) AND (cb.HR_Department_ID=$P{HR_Department_ID}) AND (sup.HR_Emp_Gate_Attendence_ID IS NULL) AND (cb.joiningdate<='$P!{WorkDate}')]]>
	</queryString>
