	<parameter name="payrollprocesstype" class="java.lang.String"/>
	<parameter name="HR_Period_ID" class="java.math.BigDecimal"/>
	<parameter name="Parent_Org_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT DISTINCT(bp.C_BPartner_ID),prs.HR_Process_ID  FROM C_BPartner bp
JOIN HR_Movement hrm ON
(bp.C_BPartner_ID = hrm.C_BPartner_ID AND hrm.HR_Process_ID=(SELECT HR_Process_ID FROM HR_Process WHERE HR_Period_ID=$P{HR_Period_ID} AND Payrollprocesstype='$P!{payrollprocesstype}'))
JOIN HR_Process prs ON (hrm.HR_Process_ID= prs.HR_Process_ID)
JOIN HR_Period period ON (prs.HR_Period_ID = period.HR_Period_ID)
 WHERE IsEmployee = 'Y' AND relievingdate is Null AND EXISTS (SELECT 1 FROM HR_Movement WHERE C_BPartner_ID = bp.C_BPartner_ID AND HR_Process_ID = hrm.HR_Process_ID )
AND joiningdate <= period.enddate
AND (CASE WHEN $P{HR_Department_ID} = 0 THEN 1=1  ELSE bp.HR_Department_ID = $P{HR_Department_ID} END)
AND (CASE WHEN $P{AD_Org_ID} = 0 THEN 1=1  ELSE bp.ad_org_ID = $P{AD_Org_ID} END)
AND (CASE WHEN $P{HR_Employee_Type_ID} = 0 THEN 1=1  ELSE bp.HR_Employee_Type_ID = $P{HR_Employee_Type_ID} END)
AND (CASE WHEN $P{C_BPartner_ID} = 0 THEN 1=1  ELSE bp.C_BPartner_ID = $P{C_BPartner_ID} END)]]>
	</queryString>
