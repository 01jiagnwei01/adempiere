	<queryString><![CDATA[SELECT total_leaves AS TotalLeaves,used_leaves AS UsedLeaves,balance_leaves AS BalanceLeaves,prvYearCFLeaves AS PreviousYearLeaves 
,hlt.leavetype AS LeaveType, bp.name AS Employee,cy.fiscalyear AS fsclyear
FROM HR_Leave_Assign hla
JOIN C_Year cy ON (cy.C_Year_ID = hla.C_Year_ID)
JOIN C_BPartner bp ON (bp.C_BPartner_ID = hla.C_BPartner_ID)
JOIN HR_LeaveType hlt ON (hlt.HR_LeaveType_ID = hla.HR_LeaveType_ID)
WHERE hla.C_BPartner_ID=$P{C_BPartner_ID} AND hla.C_Year_ID=$P{C_Year_ID}]]></queryString>

