	<parameter name="ToDate" class="java.util.Date"/>
	<parameter name="HR_Employee_Type_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[new BigDecimal(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["./"]]></defaultValueExpression>
	</parameter>
	<parameter name="Parent_Org_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT cb.name,
       cb.employee_code,
       hrd.name as designation,
       cp.dateacct,cp.payamt,
       coalesce(cp.description,' ----- ') as description ,
       (CASE WHEN ($P{HR_Employee_Type_ID}=0) THEN 'ALL'
             ELSE (SELECT name FROM HR_Employee_Type WHERE HR_Employee_Type_ID=$P{HR_Employee_Type_ID})
        END
       ) AS EmployeeTypeName
  FROM HR_Sal_Adv_Req hrs
  JOIN C_Payment cp ON hrs.HR_Sal_Adv_Req_ID =  (SELECT  cpa.payment_ref_id   FROM  C_Payment cpa  WHERE cpa.ad_table_id = 7000011  AND cp.C_Payment_ID =cpa.C_Payment_ID )
  JOIN C_BPartner cb ON hrs.C_BPartner_ID = cb.C_BPartner_ID
  JOIN HR_Designation hrd ON cb.HR_Designation_ID = hrd.HR_Designation_ID
  JOIN HR_Employee_Type hrt ON cb.HR_Employee_Type_ID = hrt.HR_Employee_Type_ID
  WHERE cp.C_Doctype_ID =   (SELECT C_Doctype_ID FROM C_Doctype WHERE name = 'AP Payment')
                      AND cp.ad_table_id = 7000011
	AND (cp.dateacct BETWEEN $P{FromDate} AND $P{ToDate})
        AND (CASE WHEN $P{HR_Employee_Type_ID} = 0 THEN 1=1  ELSE hrt.HR_Employee_Type_ID = $P{HR_Employee_Type_ID} END )

ORDER BY cb.name;]]>
	</queryString>
