	<queryString>
		<![CDATA[SELECT distinct
mi.documentNo AS IndentNumber,
mi.movementdate AS IndentDate ,
mi.movementtype As movementtype,
mp.value  AS ItemCode ,
mp.name AS ItemName ,
COALESCE(mil.qtyinternaluse,0) AS IssuedQty ,

COALESCE(sr.name,'---') AS MatchineName,
hrd.name AS Department,
( CASE WHEN $P{M_Product_Category_ID}=0 THEN 'ALL' ELSE  mpc.name END )  As financialHead ,
(COALESCE(sum(mcd.amt),0))  AS Amount

      FROM  M_Inventory mi
         JOIN  M_InventoryLine mil ON mil.M_Inventory_ID=mi.M_Inventory_ID
         JOIN  M_Product mp ON mil.M_Product_ID=mp.M_Product_ID
         JOIN  M_CostDetail mcd ON (mcd.M_InventoryLine_ID=mil.M_InventoryLine_ID ) AND (mcd.M_product_ID=mp.M_Product_ID)
         JOIN  M_Product_Category  mpc ON mpc.M_Product_Category_ID=mp.M_Product_Category_ID
         JOIN  HR_Department hrd ON hrd.HR_Department_ID=mi.HR_Department_ID
         LEFT OUTER JOIN S_Resource  sr ON sr.s_resource_id=mi.s_resource_id
         Where (CAST(mi.movementdate AS DATE ) between CAST('$P!{FromDate}' AS DATE ) and CAST('$P!{ToDate}' AS DATE ) )
           AND ( CASE WHEN $P{M_Product_Category_ID}=0 THEN 1=1 ELSE mpc.M_Product_Category_ID=$P{M_Product_Category_ID} END )
           AND (mi.movementtype IN ('I-', 'I+') )
           AND (mcd.dateacct BETWEEN '$P!{FromDate}' AND '$P!{ToDate}')
           AND mi.docstatus IN ( 'CO' , 'CL' )
           AND (mcd.C_AcctSchema_ID = $P{C_AcctSchema_ID})
           AND mcd.M_Costtype_ID  IN (SELECT M_Costtype_ID FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
           AND mcd.costingmethod  IN (SELECT costingmethod FROM  C_AcctSchema WHERE C_AcctSchema_ID = $P{C_AcctSchema_ID} )
GROUP by mi.documentNo ,mi.movementdate,mp.value, mp.name ,mil.qtyinternaluse,sr.name , hrd.name, mpc.name ,mp.M_Product_ID ,mpc.M_Product_Category_ID,mi.movementtype
Order by mi.documentNo;]]>
	</queryString>
