	</parameter>
	<queryString><![CDATA[SELECT bpemp.name AS Employee, bpsup.name AS Supervisor, goals.start_period, 
goals.end_period, goals.isAgreed, goals.period_Type FROM HR_Goals goals
JOIN C_BPartner bpemp ON goals.C_BPartner_ID = bpemp.C_BPartner_ID
JOIN C_BPartner bpsup ON goals.Supervisor_ID = bpsup.C_Bpartner_ID
WHERE 
isFinalApproved ='N'
AND isAgreed= 'Y'
AND isowndocumentapproved = 'Y'
AND ( CASE WHEN '$P!{Period_Type}' ='' THEN 1=1 ELSE Period_Type = '$P!{Period_Type}' END )
AND ( CASE WHEN $P!{Supervisor_ID} = 0 THEN 1=1  ELSE Supervisor_ID = $P!{Supervisor_ID} END )
AND ( CASE WHEN $P!{HR_Plan_Type_ID}=0 THEN 1=1 ELSE goals.HR_Plan_Type_ID = $P!{HR_Plan_Type_ID} END )
ORDER BY Supervisor_ID]]></queryString>

