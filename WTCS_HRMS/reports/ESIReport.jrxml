	<queryString>
		<![CDATA[SELECT partner.employee_code AS "EmployeeCode",partner.name AS "EmployeeName",concept.name As "CategoryName",ROUND(movement.amount) AS "ESIAmount",employee.esinumber AS "ESINo", 'Employee Contribution' AS "EmployeeContribution", '0' AS "TotalAmount",
(SELECT amount FROM HR_Movement WHERE C_BPartner_ID = employee.C_BPartner_ID AND HR_Process_ID=movement.HR_Process_ID AND HR_Concept_ID = (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='GrossSalary') ) AS "GrossSalary",process.dateacct,
(SELECT amount FROM HR_Movement WHERE C_BPartner_ID = employee.C_BPartner_ID AND HR_Process_ID=movement.HR_Process_ID AND HR_Concept_ID = (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='Present Days') ) AS "No.OFDAYS"
FROM HR_Employee employee
	JOIN C_BPartner partner ON employee.C_BPartner_ID = partner.C_Bpartner_ID
        JOIN HR_Employee_Type type ON partner.HR_Employee_Type_ID=type.HR_Employee_Type_ID
        JOIN HR_Movement movement ON partner.c_bpartner_id=movement.c_bpartner_id
        JOIN HR_Process process ON (process.HR_Process_ID=movement.HR_Process_ID )  AND (process.HR_Period_ID=$P!{HR_Period_ID})
	JOIN HR_Concept_Category cc ON (cc.HR_Concept_Category_ID = movement.HR_Concept_Category_ID)
	JOIN HR_Concept concept ON (concept.HR_Concept_ID = movement.HR_Concept_ID)
WHERE  ( CASE WHEN $P{HR_Employee_Type_ID} = 0 THEN 1=1 ELSE type.HR_Employee_Type_ID= $P{HR_Employee_Type_ID} END)
AND employee.hasoptedesi ='Y'
AND (movement.amount) > 0
AND (movement.HR_Concept_ID IN (SELECT HR_Concept_ID FROM HR_Concept hrc WHERE hrc.name='ESI' ))
order by partner.employee_code]]>
	</queryString>
