	<queryString>
		<![CDATA[SELECT DISTINCT COALESCE(mp.value ,'') as ItemCode,
 mp.name As ItemName ,
 COALESCE(cu.name,'') AS Units,
 pc.name As FinHead ,
 COALESCE(cn.catalog_number,'') AS CatLogNumber ,
 COALESCE(dr.drawing_number,'') AS DrawingNumber ,
 COALESCE( ml.value,'') AS BIN,
 COALESCE(SUM(COALESCE(st.qtyonhand,0)) ,0) AS PresentQty ,
 COALESCE(mr.level_min,0) AS MinimumLevel,
 COALESCE(mr.level_max,0) AS MaxmimumLevel
FROM M_Product mp
  JOIN M_Product_Category pc ON pc.M_Product_Category_ID=mp.M_Product_Category_ID
  LEFT OUTER JOIN WTC_Drawing_Number dr ON mp.WTC_Drawing_Number_ID=dr.WTC_Drawing_Number_ID
  LEFT OUTER JOIN WTC_Item_Catalog_Number cn ON cn.WTC_Item_Catalog_Number_ID=mp.WTC_Item_Catalog_Number_ID
  LEFT OUTER JOIN C_Uom cu ON cu.c_uom_id=mp.c_uom_id
  LEFT OUTER JOIN M_Replenish  mr ON mr.M_Product_ID = mp.M_Product_ID
  LEFT OUTER  JOIN M_Locator ml ON ml.M_Locator_ID=mr.M_Locator_ID
  LEFT OUTER JOIN M_WareHouse wh ON wh.M_WareHouse_ID=ml.M_WareHouse_ID
  LEFT OUTER JOIN M_Storage st ON  ((ml.M_Locator_ID=st.M_Locator_ID) AND (mp.M_Product_ID =st.M_Product_ID))

Group by 1,2,3,4,5,6,7,9,10
ORDER BY ItemName]]>
	</queryString>
