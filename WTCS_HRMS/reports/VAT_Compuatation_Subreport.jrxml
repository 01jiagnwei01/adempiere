 <jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="VAT_Compuatation_Subreport" pageWidth="595" pageHeight="842" whenNoDataType="NoDataSection" columnWidth="595" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="isSoTrx" class="java.lang.String"/>
	<parameter name="FromDate" class="java.sql.Timestamp"/>
	<parameter name="ToDate" class="java.sql.Timestamp"/>
	<parameter name="AD_Client_ID" class="java.math.BigDecimal"/>
	<parameter name="ExcessCredit" class="java.math.BigDecimal"/>
	<parameter name="WTC_TaxIndicator_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT SUM( @ cit.taxAmt) AS VATclaimed,SUM( @ cit.taxbaseamt) as excludingVAT,ct.name ,ct.rate
FROM C_InvoiceTax cit
JOIN C_Invoice c ON (c.C_Invoice_ID=cit.C_Invoice_ID)
JOIN C_Tax ct ON ( ct.C_Tax_ID=cit.C_Tax_ID)
JOIN WTC_TaxIndicator wti ON ( wti.WTC_TaxIndicator_ID= ct.WTC_TaxIndicator_ID)
WHERE ct.WTC_TaxIndicator_ID= $P{WTC_TaxIndicator_ID}
AND (
CASE WHEN $P{isSoTrx}='Y' THEN
   ( CASE WHEN wti.collectableon ='SO' THEN
        c.C_DocType_ID IN (select cd.C_DocType_ID from C_DocType cd where cd.docbasetype in ('ARI','APC') )
   ELSE
	( CASE WHEN ct.isSalesTax ='N' THEN  c.C_DocType_ID IN (select cd.C_DocType_ID from C_DocType cd where cd.docbasetype in ('API','ARC') )
	ELSE
	1=2
	END) END )
ELSE
  ( CASE WHEN ct.isSalesTax ='N' THEN
      (  CASE WHEN wti.collectableon ='PO' THEN
           c.C_DocType_ID IN (select cd.C_DocType_ID from C_DocType cd where cd.docbasetype in ('ARI','APC') )
         ELSE
	 ( CASE WHEN ct.isSalesTax ='N' THEN  c.C_DocType_ID IN (select cd.C_DocType_ID from C_DocType cd where cd.docbasetype in ('API','ARC') )
	ELSE
	1=2
	END) END
      )
   ELSE
     1=2
   END )

END )
AND c.dateInvoiced >= $P{FromDate} AND c.dateInvoiced <= $P{ToDate}
AND c.ad_client_id = $P{AD_Client_ID}
AND c.DocStatus IN ('CO','CL')
GROUP BY ct.name,ct.rate]]>
	</queryString>
	<field name="vatclaimed" class="java.math.BigDecimal"/>
	<field name="excludingvat" class="java.math.BigDecimal"/>
	<field name="name" class="java.lang.String"/>
	<field name="rate" class="java.math.BigDecimal"/>
	<variable name="TatalAssessableValue" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{excludingvat}]]></variableExpression>
	</variable>
	<variable name="TotalTaxAmount" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{vatclaimed}]]></variableExpression>
	</variable>
	<variable name="ReturnValue" class="java.math.BigDecimal">
		<variableExpression><![CDATA[( $P{isSoTrx}.equals("N") ? $V{TotalTaxAmount}.add($P{ExcessCredit})  : $V{TotalTaxAmount} )]]></variableExpression>
		<initialValueExpression><![CDATA[java.math.BigDecimal.ZERO]]></initialValueExpression>
	</variable>
