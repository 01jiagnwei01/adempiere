	<parameter name="ToDate" isForPrompting="true" class="java.sql.Timestamp"/>
	<parameter name="C_BPartner_ID" isForPrompting="true" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[new BigDecimal(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="HR_Employee_Type_ID" isForPrompting="true" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[new BigDecimal(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" isForPrompting="false" class="java.lang.String">
