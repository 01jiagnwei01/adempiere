	<queryString><![CDATA[SELECT DISTINCT COALESCE(cb.name,' ') as employeename, COALESCE(cb.employee_code,' ') as employeenumber,
cb.C_BPartner_ID,
(CASE WHEN ($P{HR_Employee_Type_ID}=0) THEN 'ALL' ELSE (SELECT name FROM HR_Employee_Type WHERE HR_Employee_Type_ID=$P{HR_Employee_Type_ID})   END) AS EmployeeTypeName ,
(SELECT COALESCE(sum(tok.totalamount),0) FROM HR_Emp_Token tok WHERE ( tok.C_BPartner_ID=token.C_BPartner_ID )  AND (tok.issuedate  BETWEEN '$P!{FromDate}' AND '$P!{ToDate}') ) AS total
FROM HR_Emp_Token token
JOIN C_BPartner cb ON token.C_BPartner_ID=cb.C_BPartner_ID
WHERE
 (CASE WHEN $P{C_BPartner_ID} = 0 THEN 1=1  ELSE token.C_BPartner_id=$P{C_BPartner_ID} END)
AND (token.issuedate  BETWEEN '$P!{FromDate}' AND '$P!{ToDate}')
AND
 (CASE WHEN $P{HR_Employee_Type_ID} = 0 THEN 1 = 1 ELSE cb.HR_Employee_Type_ID=$P{HR_Employee_Type_ID} END)]]></queryString>

