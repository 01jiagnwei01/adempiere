	<parameter name="Parent_Org_ID" class="java.math.BigDecimal"/>
	<parameter name="M_PriceList_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT mp.name AS itemName, COALESCE(vb.breakvalue,0) AS qtyOrdered, bp.name AS suppliername , COALESCE(vb.pricestd,0) AS price

FROM M_Product mp JOIN C_OrderLine col  ON col.M_Product_ID=mp.M_product_ID
                  JOIN C_Order co  ON co.C_Order_ID=col.C_Order_ID
                  JOIN C_BPartner bp  ON co.C_BPartner_ID= bp.C_BPartner_ID


                  JOIN m_productpricevendorbreak vb ON ((bp.C_BPartner_ID=vb.C_BPartner_ID)  AND  (mp.M_Product_ID=vb.M_Product_ID))
WHERE vb.M_priceList_Version_ID IN ( SELECT M_PriceList_Version_ID FROM M_PriceList_Version WHERE M_PriceList_ID =$P{M_PriceList_ID} and isactive='Y') AND bp.C_BPartner_ID in (SELECT t_selection_id FROM T_Selection WHERE ad_pinstance_id =$P{AD_PInstance_ID}  )  and (CASE WHEN $P{M_Product_ID} =0 THEN 1=1 ELSE mp.m_product_id = $P{M_Product_ID} END)]]>
	</queryString>
