	<parameter name="WorkDate" class="java.sql.Timestamp"/>
	<parameter name="HR_Employee_Type_ID" class="java.math.BigDecimal"/>
	<parameter name="HR_Work_Shift_ID" class="java.math.BigDecimal"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["./"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT Distinct
     (Base.Org) AS Organization ,
     (Base.department) AS Department,
     (SELECT  name FROM HR_Employee_Type WHERE HR_Employee_Type_ID=$P{HR_Employee_Type_ID}) AS EmplyeeType,
     ( SELECT  name FROM HR_Work_Shift WHERE HR_Work_Shift_ID=$P{HR_Work_Shift_ID} ) AS WorkShift,
     (
       SELECT COUNT( cb1.C_BPartner_ID)
             FROM C_BPartner cb1
             JOIN HR_Work_Shift  ws1 ON ws1.HR_Work_Shift_ID=$P{HR_Work_Shift_ID}
             JOIN HR_Shift_Alloc alc1 ON (alc1.HR_Work_Shift_ID=ws1.HR_Work_Shift_ID) AND (alc1.workdate='$P!{WorkDate}')
             JOIN HR_Work_Group wg1  ON  (wg1.HR_Work_Group_ID =alc1.HR_Work_Group_ID ) AND (wg1.HR_Work_Group_ID=cb1.HR_Work_Group_ID)
             JOIN HR_Employee_Type empt1 ON ( empt1.HR_Employee_Type_ID=cb1.HR_Employee_Type_ID ) AND (cb1.HR_Employee_Type_ID=$P{HR_Employee_Type_ID})
       WHERE (CASE WHEN (cb1.relievingdate IS NULL) THEN (cb1.relieveemployee='N') ELSE (cb1.relievingdate > '$P!{WorkDate}') END )  AND (cb1.isemployee='Y' ) AND (cb1.division_ID=Base.AD_Org_ID) AND (cb1.HR_Department_ID =Base.HR_Department_ID )  AND (cb1.joiningdate<='$P!{WorkDate}')
     ) AS ONRoll,
     (
       SELECT COALESCE(dept2.stregthrequired,0) FROM HR_Department dept2 WHERE dept2.HR_Department_ID=Base.HR_Department_ID
     ) AS Required,
     (
       SELECT COUNT( cb2.C_BPartner_ID)
             FROM C_BPartner cb2
             JOIN HR_Work_Shift  ws2 ON ws2.HR_Work_Shift_ID=$P{HR_Work_Shift_ID}
             JOIN HR_Shift_Alloc alc2 ON (alc2.HR_Work_Shift_ID=ws2.HR_Work_Shift_ID) AND (alc2.workdate='$P!{WorkDate}')
             JOIN HR_Work_Group wg2  ON  (wg2.HR_Work_Group_ID =alc2.HR_Work_Group_ID ) AND (wg2.HR_Work_Group_ID=cb2.HR_Work_Group_ID)
             JOIN HR_Employee_Type empt2 ON ( empt2.HR_Employee_Type_ID=cb2.HR_Employee_Type_ID ) AND (empt2.HR_Employee_Type_ID=$P{HR_Employee_Type_ID})
             JOIN HR_Emp_Sup_Attendence sup2 ON (sup2.C_BPartner_ID=cb2.C_BPartner_ID) AND (sup2.workdate='$P!{WorkDate}') AND (sup2.actual_work_shift_id=$P{HR_Work_Shift_ID})
       WHERE (CASE WHEN (cb2.relievingdate IS NULL) THEN (cb2.relieveemployee='N') ELSE (cb2.relievingdate > '$P!{WorkDate}') END )  AND ( cb2.isemployee='Y' ) AND (cb2.division_ID=Base.AD_Org_ID) AND (cb2.HR_Department_ID =Base.HR_Department_ID )  AND (cb2.joiningdate<='$P!{WorkDate}') AND (sup2.actual_work_shift_id=$P{HR_Work_Shift_ID})  AND (sup2.workdate='$P!{WorkDate}')
     ) AS Present,
     (
       SELECT COUNT(cb4.C_BPartner_ID)
              FROM C_BPartner cb4
              JOIN HR_Work_Shift  ws4 ON ws4.HR_Work_Shift_ID=$P{HR_Work_Shift_ID}
              JOIN HR_Shift_Alloc alc4 ON (alc4.HR_Work_Shift_ID=ws4.HR_Work_Shift_ID) AND (alc4.workdate='$P!{WorkDate}')
              JOIN HR_Work_Group wg4  ON  (wg4.HR_Work_Group_ID =alc4.HR_Work_Group_ID ) AND (wg4.HR_Work_Group_ID=cb4.HR_Work_Group_ID)
              LEFT OUTER JOIN HR_Emp_Sup_Attendence sup4 ON ( cb4.C_BPartner_ID=sup4.C_BPartner_ID )
              AND  (CASE WHEN (sup4.HR_Emp_Sup_Attendence_ID IS NULL) THEN (1=1)
                         WHEN (sup4.HR_Emp_Sup_Attendence_ID IS NOT NULL) THEN (CASE WHEN  (sup4.actual_work_shift_id=$P{HR_Work_Shift_ID}) THEN (1=2) ELSE (1=1) END)
                         ELSE (1=2)
                    END
                   )
               AND (sup4.workdate='$P!{WorkDate}')
              WHERE (CAST(cb4.weeklyoff as double precision) ) =(CASE WHEN (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') ) =0 THEN 7
                                                                      ELSE (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') ) END)
               AND (cb4.HR_Employee_Type_ID=$P{HR_Employee_Type_ID})
               AND (CASE WHEN (cb4.relievingdate IS NULL) THEN (cb4.relieveemployee='N') ELSE (cb4.relievingdate > '$P!{WorkDate}') END)
               AND (cb4.isemployee='Y' ) AND (cb4.division_ID=Base.AD_Org_ID)
               AND (cb4.HR_Department_ID =Base.HR_Department_ID )  AND (cb4.joiningdate<='$P!{WorkDate}')   AND (sup4.workdate='$P!{WorkDate}')
               AND  (CASE WHEN (sup4.HR_Emp_Sup_Attendence_ID IS NULL) THEN (1=1)
                         WHEN (sup4.HR_Emp_Sup_Attendence_ID IS NOT NULL) THEN (CASE WHEN  (sup4.actual_work_shift_id=$P{HR_Work_Shift_ID}) THEN (1=2) ELSE (1=1) END)
                         ELSE (1=2)
                    END
                   )

     ) AS WeeklyOff,
     (
       SELECT COUNT(lr3.HR_Leave_Request_ID)
              FROM C_BPartner cb3
              JOIN HR_Work_Shift  ws3 ON ws3.HR_Work_Shift_ID=$P{HR_Work_Shift_ID}
              JOIN HR_Shift_Alloc alc3 ON (alc3.HR_Work_Shift_ID=ws3.HR_Work_Shift_ID) AND (alc3.workdate='$P!{WorkDate}')
              JOIN HR_Work_Group wg3  ON  (wg3.HR_Work_Group_ID =alc3.HR_Work_Group_ID ) AND (wg3.HR_Work_Group_ID=cb3.HR_Work_Group_ID)
              JOIN HR_Leave_Request lr3 ON lr3.C_BPartner_ID=cb3.C_BPartner_ID
              JOIN HR_Department dep3 ON dep3.HR_Department_ID=cb3.HR_Department_ID
              JOIN AD_Org org3 ON org3.AD_Org_ID=cb3.division_ID
              LEFT OUTER JOIN HR_Emp_Sup_Attendence sup3 ON ( cb3.C_BPartner_ID=sup3.C_BPartner_ID )
              AND  (CASE WHEN (sup3.HR_Emp_Sup_Attendence_ID IS NULL) THEN (1=1)
                         WHEN (sup3.HR_Emp_Sup_Attendence_ID IS NOT NULL) THEN (CASE WHEN  (sup3.actual_work_shift_id=$P{HR_Work_Shift_ID}) THEN (1=2) ELSE (1=1) END)
                         ELSE (1=2)
                    END
                   )
              AND (sup3.workdate='$P!{WorkDate}')
       WHERE (Case WHEN ((CAST(cb3.weeklyoff as double precision) ) =(CASE WHEN (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') ) =0 THEN 7
                                                                      ELSE (SELECT EXTRACT(DOW FROM TIMESTAMP '$P!{WorkDate}') ) END) ) THEN 1=2 ELSE 1=1 END)
          AND (CASE WHEN (cb3.relievingdate IS NULL) THEN (cb3.relieveemployee='N') ELSE (cb3.relievingdate > '$P!{WorkDate}') END )
          AND ( cb3.isemployee='Y' )
          AND (cb3.division_ID=Base.AD_Org_ID)
          AND (cb3.HR_Department_ID =Base.HR_Department_ID )
          AND ('$P!{WorkDate}' BETWEEN lr3.fromdate AND lr3.todate) AND (lr3.isapproved='Y')  AND (cb3.joiningdate<='$P!{WorkDate}')  AND (sup3.workdate='$P!{WorkDate}')
          AND  (CASE WHEN (sup3.HR_Emp_Sup_Attendence_ID IS NULL) THEN (1=1)
                         WHEN (sup3.HR_Emp_Sup_Attendence_ID IS NOT NULL) THEN (CASE WHEN  (sup3.actual_work_shift_id=$P{HR_Work_Shift_ID}) THEN (1=2) ELSE (1=1) END)
                         ELSE (1=2)
                    END
               )

     ) AS Leaves
  FROM
    (SELECT  ado.AD_Org_ID ,ado.name AS Org ,depart.HR_Department_ID ,depart.name  As department
      FROM AD_Org ado JOIN C_BPartner cpart ON cpart.division_ID=ado.AD_Org_ID
                      JOIN HR_Department depart ON depart.HR_Department_ID=cpart.HR_Department_ID
      WHERE   (CASE WHEN (cpart.relievingdate IS NULL) THEN (cpart.relieveemployee='N') ELSE (cpart.relievingdate > '$P!{WorkDate}') END )  AND (cpart.isemployee='Y' )
    ) AS Base
 Order By Organization]]>
	</queryString>
