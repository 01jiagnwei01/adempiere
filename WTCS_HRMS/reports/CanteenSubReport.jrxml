<!DOCTYPE jasperReport PUBLIC "//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport
		 name="CanteenSubReport"
		 columnCount="1"
		 printOrder="Horizontal"
		 orientation="Portrait"
		 pageWidth="350"
		 pageHeight="842"
		 columnWidth="350"
		 columnSpacing="0"
		 leftMargin="0"
		 rightMargin="0"
		 topMargin="0"
		 bottomMargin="0"
		 whenNoDataType="NoPages"
		 isTitleNewPage="false"
		 isSummaryNewPage="false">
	<property name="ireport.zoom" value="1.0" />
	<property name="ireport.x" value="0" />
	<property name="ireport.y" value="0" />
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="C_BPartner_ID" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="FromDate" isForPrompting="true" class="java.sql.Timestamp"/>
	<parameter name="ToDate" isForPrompting="true" class="java.sql.Timestamp"/>
	<queryString><![CDATA[SELECT  COALESCE(ht.name,' ') as tokenName ,
(CASE WHEN ht.tokenlevel ='CA'  THEN 'Canteen' ELSE 'Store' END) AS TokenLevel ,
sum(COALESCE(het.nooftokens,0)) as tokenCount ,
sum(COALESCE(het.totalamount,0)) as  totalAmount

FROM hr_token ht
JOIN hr_emp_token het on ht.hr_token_id=het.hr_token_id
WHERE het.c_bpartner_id=$P{C_BPartner_ID} AND het.issuedate between '$P!{FromDate}' and '$P!{ToDate}'
Group by tokenName ,ht.tokenlevel
Order By ht.tokenlevel]]></queryString>

	<field name="tokenName" class="java.lang.String"/>
	<field name="tokenCount" class="java.math.BigDecimal"/>
	<field name="totalAmount" class="java.math.BigDecimal"/>
	<field name="tokenlevel" class="java.lang.String"/>

