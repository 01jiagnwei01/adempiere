	<queryString>
		<![CDATA[SELECT cs.tax_division,cs.tax_circle,cs.registration_number,c.name,cs.registered_address ,vp.C_Period_ID,vp.startdate,
op.enddate
FROM WTC_ClientStatutory cs, AD_Client c ,

( SELECT cp1.C_Period_ID,cp1.startdate
    FROM C_Period cp1
    WHERE cp1.C_Year_ID = (SELECT cc.C_Year_ID
                           FROM C_Year cc
                           WHERE  cc.fiscalyear= CAST(date_part('year',CAST($P{FromDate} as date)) as text)
                           )
    AND  ( $P{FromDate} BETWEEN cp1.startdate and cp1.enddate )
)  vp ,

( SELECT cp.enddate
   FROM C_Period cp
   WHERE cp.C_Year_ID = ( SELECT cc1.C_Year_ID
                            FROM C_Year cc1
                            WHERE cc1.fiscalyear= CAST(date_part('year',CAST($P{ToDate} as date) ) as text)
                        )
    AND ( $P{ToDate} BETWEEN cp.startdate and cp.enddate )
)  op

WHERE cs.registration_name = 'VAT' AND c.AD_Client_ID=$P{AD_Client_ID} AND c.AD_Client_ID=cs.AD_Client_ID;]]>
	</queryString>
